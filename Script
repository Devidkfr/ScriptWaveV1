-- Scripts:

local function NCEMLR_fake_script() -- Fly.LocalScript 
	local script = Instance.new('LocalScript', Fly)

	local UIS = game:GetService("UserInputService")
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	local flying = false
	local speed = 2  -- You can adjust the speed here
	
	-- Variables for key states
	local moveForward = false
	local moveBackward = false
	local moveLeft = false
	local moveRight = false
	
	-- Function to start flying
	local function startFlying()
		flying = true
		humanoidRootPart.Anchored = true  -- Prevent falling or physics effects
	end
	
	-- Function to stop flying
	local function stopFlying()
		flying = false
		humanoidRootPart.Anchored = false  -- Re-enable physics
	end
	
	-- Toggle flying when the hotkey (F) is pressed
	UIS.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.F then
			if not flying then
				startFlying()
			else
				stopFlying()
			end
		end
	
		-- Detect movement keys
		if input.KeyCode == Enum.KeyCode.W then
			moveForward = true
		elseif input.KeyCode == Enum.KeyCode.S then
			moveBackward = true
		elseif input.KeyCode == Enum.KeyCode.A then
			moveLeft = true
		elseif input.KeyCode == Enum.KeyCode.D then
			moveRight = true
		end
	end)
	
	-- Stop moving when keys are released
	UIS.InputEnded:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.W then
			moveForward = false
		elseif input.KeyCode == Enum.KeyCode.S then
			moveBackward = false
		elseif input.KeyCode == Enum.KeyCode.A then
			moveLeft = false
		elseif input.KeyCode == Enum.KeyCode.D then
			moveRight = false
		end
	end)
	
	-- Continuous movement control using RenderStepped and CFrame
	game:GetService("RunService").RenderStepped:Connect(function()
		if flying then
			local moveDirection = Vector3.new(0, 0, 0)
			if moveForward then
				moveDirection = moveDirection + humanoidRootPart.CFrame.LookVector
			end
			if moveBackward then
				moveDirection = moveDirection - humanoidRootPart.CFrame.LookVector
			end
			if moveLeft then
				moveDirection = moveDirection - humanoidRootPart.CFrame.RightVector
			end
			if moveRight then
				moveDirection = moveDirection + humanoidRootPart.CFrame.RightVector
			end
	
			-- Apply movement using CFrame for smooth motion
			humanoidRootPart.CFrame = humanoidRootPart.CFrame + moveDirection * speed
		end
	end)
	
end
coroutine.wrap(NCEMLR_fake_script)()
local function ZJJNHQF_fake_script() -- cspeed.LocalScript 
	local script = Instance.new('LocalScript', cspeed)

	local UIS = game:GetService("UserInputService")
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
	local speedButton = script.Parent  -- Reference to the button named "cspeed"
	local defaultWalkSpeed = 16  -- Default walking speed in Roblox
	local fastWalkSpeed = 150  -- Fast walking speed
	local speedActive = false  -- Speed state (on/off)
	
	-- Function to toggle speed
	local function toggleSpeed()
		speedActive = not speedActive
		if speedActive then
			speedButton.Text = "Deactivate Speed"  -- Update button text
			print("Speed Activated")  -- Debug print
		else
			speedButton.Text = "Activate Speed"  -- Update button text
			print("Speed Deactivated")  -- Debug print
		end
	end
	
	-- Continuous movement control using RenderStepped and CFrame
	game:GetService("RunService").RenderStepped:Connect(function()
		if speedActive then
			local moveDirection = Vector3.new(0, 0, 0)
	
			-- Check for movement keys
			if UIS:IsKeyDown(Enum.KeyCode.W) then
				moveDirection = moveDirection + humanoidRootPart.CFrame.LookVector
			end
			if UIS:IsKeyDown(Enum.KeyCode.S) then
				moveDirection = moveDirection - humanoidRootPart.CFrame.LookVector
			end
			if UIS:IsKeyDown(Enum.KeyCode.A) then
				moveDirection = moveDirection - humanoidRootPart.CFrame.RightVector
			end
			if UIS:IsKeyDown(Enum.KeyCode.D) then
				moveDirection = moveDirection + humanoidRootPart.CFrame.RightVector
			end
	
			-- Normalize the direction and apply the speed
			if moveDirection.Magnitude > 0 then
				moveDirection = moveDirection.Unit * fastWalkSpeed
				humanoidRootPart.CFrame = humanoidRootPart.CFrame + moveDirection * game:GetService("RunService").Heartbeat:Wait()
			end
		end
	end)
	
	-- Hotkey to toggle speed (G key)
	UIS.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.G then
			toggleSpeed()
		end
	end)
	
	-- Button click to toggle speed
	speedButton.MouseButton1Click:Connect(function()
		toggleSpeed()
	end)
	
end
coroutine.wrap(ZJJNHQF_fake_script)()
local function BJJOAFM_fake_script() -- cinv.LocalScript 
	local script = Instance.new('LocalScript', cinv)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	
	-- Function to make the character invisible
	local function makeInvisible()
		for _, part in ipairs(character:GetChildren()) do
			if part:IsA("BasePart") then
				part.Transparency = 1  -- Make the part invisible
			end
		end
		character:FindFirstChildOfClass("Humanoid").HipWidth = 0  -- Optional: make humanoid not collide
		character:FindFirstChildOfClass("Humanoid").HipHeight = 0 -- Optional: make humanoid not collide
	end
	
	-- Button click to make the humanoid invisible
	script.Parent.MouseButton1Click:Connect(makeInvisible)
	
end
coroutine.wrap(BJJOAFM_fake_script)()
local function UBSGIV_fake_script() -- invfix.LocalScript 
	local script = Instance.new('LocalScript', invfix)

	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	
	-- Function to make the character visible again
	local function makeVisible()
		for _, part in ipairs(character:GetChildren()) do
			if part:IsA("BasePart") then
				part.Transparency = 0  -- Make the part visible again
			end
		end
		character:FindFirstChildOfClass("Humanoid").HipWidth = 0.5  -- Optional: restore humanoid collision
		character:FindFirstChildOfClass("Humanoid").HipHeight = 2 -- Optional: restore humanoid collision
	end
	
	-- Button click to make the humanoid visible again
	script.Parent.MouseButton1Click:Connect(makeVisible)
	
end
coroutine.wrap(UBSGIV_fake_script)()
local function KUOXYIL_fake_script() -- Skybox.LocalScript 
	local script = Instance.new('LocalScript', Skybox)

	local button = script.Parent
	
	-- List of skybox asset IDs (replace with your own valid Skybox Asset IDs)
	local skyboxes = {
		"rbxassetid://12535312655",  -- Replace with actual Skybox Asset IDs
		"rbxassetid://8685354621",
		"rbxassetid://7365710873"
	}
	
	-- Function to change the skybox
	local function changeSkybox()
		local lighting = game:GetService("Lighting")
	
		-- Create a new Sky instance
		local newSky = Instance.new("Sky")
	
		-- Set the skybox textures randomly from the list
		newSky.SkyboxBk = skyboxes[math.random(1, #skyboxes)]
		newSky.SkyboxDn = skyboxes[math.random(1, #skyboxes)]
		newSky.SkyboxFt = skyboxes[math.random(1, #skyboxes)]
		newSky.SkyboxLf = skyboxes[math.random(1, #skyboxes)]
		newSky.SkyboxRt = skyboxes[math.random(1, #skyboxes)]
		newSky.SkyboxUp = skyboxes[math.random(1, #skyboxes)]
	
		-- If a Sky instance already exists, remove it
		if lighting:FindFirstChildOfClass("Sky") then
			lighting:FindFirstChildOfClass("Sky"):Destroy()
		end
	
		-- Assign the new sky to the Lighting service
		newSky.Parent = lighting  -- Parent the new sky to Lighting
	end
	
	-- Button click to change the skybox
	button.MouseButton1Click:Connect(changeSkybox)
	
end
coroutine.wrap(KUOXYIL_fake_script)()
local function EXACX_fake_script() -- TrashV1.LocalScript 
	local script = Instance.new('LocalScript', TrashV1)

	local SGui = script.Parent
	local Frame = SGui:WaitForChild("Frame")
	local Button = SGui:WaitForChild("TextButton")
	
	Button.MouseButton1Up:Connect(function()
		Frame.Visible = not Frame.Visible
	end)
end
coroutine.wrap(EXACX_fake_script)()
